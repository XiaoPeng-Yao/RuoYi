             <if test="applyerComp != null  and applyerComp != '' "> and APPLYER_COMP = #{applyerComp}</if>
             <if test="expireDate != null "> and EXPIRE_DATE = #{expireDate}</if>
             <if test="effectDate != null "> and EFFECT_DATE = #{effectDate}</if>
             <if test="claimDate != null "> and CLAIM_DATE = #{claimDate}</if>
             <if test="completeDate != null "> and COMPLETE_DATE = #{completeDate}</if>
             <if test="savePath != null  and savePath != '' "> and SAVE_PATH = #{savePath}</if>
             <if test="gmtCreate != null "> and GMT_CREATE = #{gmtCreate}</if>
             <if test="createOperId != null  and createOperId != '' "> and CREATE_OPER_ID = #{createOperId}</if>
             <if test="gmtModified != null "> and GMT_MODIFIED = #{gmtModified}</if>
             <if test="modifiedOperId != null  and modifiedOperId != '' "> and MODIFIED_OPER_ID = #{modifiedOperId}</if>
             <if test="gmtAudit != null "> and GMT_AUDIT = #{gmtAudit}</if>
             <if test="auditOperId != null  and auditOperId != '' "> and AUDIT_OPER_ID = #{auditOperId}</if>
         </where>
    </select>
    
    <select id="selectWishInfoById" parameterType="Integer" resultMap="WishInfoResult">
        <include refid="selectWishInfoVo"/>
        where WISH_ID = #{wishId}
    </select>
        
