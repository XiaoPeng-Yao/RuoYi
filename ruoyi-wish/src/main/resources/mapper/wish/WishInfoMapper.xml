			<if test="modifiedOperId != null  and modifiedOperId != ''  ">#{modifiedOperId},</if>
			<if test="gmtAudit != null  ">#{gmtAudit},</if>
			<if test="auditOperId != null  and auditOperId != ''  ">#{auditOperId},</if>
