	<sql id="selectWishInfoVo">
        select WISH_ID, WISH_NAME, WISH_ORG, WISH_STATUS, WISH_TARGET, WISH_TYPE, WISH_COUNT, IS_RECOMMEND, WISH_PROGRESS, WISH_DESC, APPLYER_NAME, APPLYER__PHONE, APPLYER_COMP, EXPIRE_DATE, EFFECT_DATE, CLAIM_DATE, COMPLETE_DATE, SAVE_PATH, GMT_CREATE, CREATE_OPER_ID, GMT_MODIFIED, MODIFIED_OPER_ID, GMT_AUDIT, AUDIT_OPER_ID from wish_info
    </sql>
	
    <select id="selectWishInfoList" parameterType="WishInfo" resultMap="WishInfoResult">
        <include refid="selectWishInfoVo"/>
        <where>  
            <if test="wishId != null "> and WISH_ID = #{wishId}</if>
             <if test="wishName != null  and wishName != '' "> and WISH_NAME = #{wishName}</if>
             <if test="wishOrg != null  and wishOrg != '' "> and WISH_ORG = #{wishOrg}</if>
             <if test="wishStatus != null  and wishStatus != '' "> and WISH_STATUS = #{wishStatus}</if>
             <if test="wishTarget != null  and wishTarget != '' "> and WISH_TARGET = #{wishTarget}</if>
             <if test="wishType != null  and wishType != '' "> and WISH_TYPE = #{wishType}</if>
             <if test="wishCount != null "> and WISH_COUNT = #{wishCount}</if>
             <if test="isRecommend != null  and isRecommend != '' "> and IS_RECOMMEND = #{isRecommend}</if>
             <if test="wishProgress != null  and wishProgress != '' "> and WISH_PROGRESS = #{wishProgress}</if>
             <if test="wishDesc != null  and wishDesc != '' "> and WISH_DESC = #{wishDesc}</if>
             <if test="applyerName != null  and applyerName != '' "> and APPLYER_NAME = #{applyerName}</if>
